{
  "name": "diffengine",
  "version": "1.0.3",
  "description": "Compare two objects and track granular differences",
  "type": "module",
  "main": "dist/index.min.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && esbuild dist/index.js --minify --outfile=dist/index.min.js",
    "prepare": "npm run build"
  },
  "keywords": [
    "diff",
    "object-diff",
    "differences",
    "Compare",
    "kyc",
    "json",
    "objectDifference"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.min.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/index.min.js",
    "dist/index.d.ts",
    "README.md"
  ],
  "author": "harishkumar",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.15.17",
    "esbuild": "^0.25.5",
    "typescript": "^5.8.3"
  }
}
